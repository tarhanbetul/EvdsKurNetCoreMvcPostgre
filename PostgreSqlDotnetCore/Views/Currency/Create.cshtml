
@using PostgreSqlDotnetCore.Models.Evds;
@model ParaBirimi;


<form method="post" asp-controller="Currency" asp-action="Create">
    <div class="form-row">
        <div class="form-group col-md-6">
            <label for="inputEmail4">Currency Name</label>
            <input type="text" class="form-control" id="inputEmail4" placeholder="Name" asp-for="Name">
        </div>
        <div class="form-group col-md-6">
            <label for="inputPassword4">Currency Country</label>
            <input type="text" class="form-control" id="inputPassword4" placeholder="Country" asp-for="Country">
        </div>
    </div>
    <div class="form-group">
        <label for="inputAddress">Currency Content</label>
        <input type="text" class="form-control" id="inputAddress" placeholder="Content" asp-for="Content">
    </div>

    <button type="submit" class="btn btn-primary">Create</button>
</form>
